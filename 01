#include<stdio.h>

int main(){
    int A [8];
    int i;
    for(i = 0; i < 8; i++){
        printf("Informe o valor da posicao %d\n", i);
        scanf("%d", &A[i]);
    } 
        printf("Valores armazenados na ordem inversa:\n");
    for(i = 7; i >= 0; i--){
       
        printf("%d ", A[i]);
    }
       return 0;
}
